{
  "name": "nodepop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=nodepop:* node ./node_modules/pm2/bin/pm2 start",
    "log": "node ./node_modules/pm2/bin/pm2 log",
    "stop": "node ./node_modules/pm2/bin/pm2 stop all",
    "prod": "node ./node_modules/pm2/bin/pm2 start --env production",
    "initdb": "node install_db.js",
    "test": "mocha **/*.e2e.js --exit"
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "cookie-parser": "~1.4.3",
    "cote": "^0.19.1",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ejs": "~2.5.7",
    "express": "~4.18.2",
    "http-errors": "~1.6.2",
    "i18n": "^0.8.3",
    "jimp": "^0.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "pm2": "^3.4.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "mocha": "^6.2.0",
    "supertest": "^4.0.2"
  }
}
